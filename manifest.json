{
"name": "ProductiVision",
"description": "This is a new tab page for productivity",
"version": "1.0.0",
"manifest_version": 2,
"chrome_url_overrides" : {
    "newtab": "index.html"
  },
"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/bootstrap.css", "css/style"],
      "js": ["js/production.js"]
    }],
"web_accessible_resources": ["js/production.js"]
}
